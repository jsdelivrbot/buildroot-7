From 194761c928fb3b3d06294dce8062d4b0bd1c072e Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Thu, 19 Mar 2015 18:03:48 +0100
Subject: [PATCH] ARM: DT: ls1021a-tqmls1021a-mbls102x: add port expander 1

Add expander pca9554 @20 to i2c0

The expander uses IRQ4: ID 200 - 32 -> 168
---
 arch/arm/boot/dts/ls1021a-tqmls1021a-mbls102x.dts | 7 +++++++
 1 file changed, 7 insertions(+)

diff --git a/arch/arm/boot/dts/ls1021a-tqmls1021a-mbls102x.dts b/arch/arm/boot/dts/ls1021a-tqmls1021a-mbls102x.dts
index 767370f..f11a732 100644
--- a/arch/arm/boot/dts/ls1021a-tqmls1021a-mbls102x.dts
+++ b/arch/arm/boot/dts/ls1021a-tqmls1021a-mbls102x.dts
@@ -65,6 +65,13 @@
 
 &i2c0 {
 	status = "okay";
+	pca9554_0: pca954@20 {
+		compatible = "nxp,pca9554";
+		reg = <0x20>;
+/*		interrupts = <GIC_SPI 168 IRQ_TYPE_LEVEL_HIGH>; */
+		status = "okay";
+	};
+
 	pca9530: pca9530@60 {
 		compatible = "nxp,pca9530";
 		reg = <0x60>;
