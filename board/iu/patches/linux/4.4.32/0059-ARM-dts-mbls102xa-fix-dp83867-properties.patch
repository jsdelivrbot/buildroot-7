From 790e967e409639694441c32df336cf1cd86b533a Mon Sep 17 00:00:00 2001
From: Michael Krummsdorf <michael.krummsdorf@tq-group.com>
Date: Tue, 1 Mar 2016 18:28:52 +0100
Subject: [PATCH] ARM: dts: mbls102xa: fix dp83867 properties

Signed-off-by: Michael Krummsdorf <michael.krummsdorf@tq-group.com>
---
 arch/arm/boot/dts/ls1021a-tqmls1021a-mbls102x.dts | 14 ++++++--------
 1 file changed, 6 insertions(+), 8 deletions(-)

diff --git a/arch/arm/boot/dts/ls1021a-tqmls1021a-mbls102x.dts b/arch/arm/boot/dts/ls1021a-tqmls1021a-mbls102x.dts
index db181e6..a80e444 100644
--- a/arch/arm/boot/dts/ls1021a-tqmls1021a-mbls102x.dts
+++ b/arch/arm/boot/dts/ls1021a-tqmls1021a-mbls102x.dts
@@ -220,10 +220,9 @@
 		reg = <0x05>;
 		ti,rx-internal-delay = <DP83867_RGMIIDCTL_1_75_NS>;
 		ti,tx-internal-delay = <DP83867_RGMIIDCTL_1_75_NS>;
-		rx-skew-ps = <1750>;
-		tx-skew-ps = <1750>;
-		led-cfg1 = <0x6b90>;
-		led-cfg2 = <0x0000>;
+		ti,led-cfg1 = <0x6b90>;
+		ti,led-cfg2 = <0x0000>;
+		ti,fifo-depth = <DP83867_PHYCR_FIFO_DEPTH_4_B_NIB>;
 	};
 	rgmii_phy0d: ethernet-phy@0d {
 		compatible = "ethernet-phy-id2000.a230",
@@ -231,10 +230,9 @@
 		reg = <0x0d>;
 		ti,rx-internal-delay = <DP83867_RGMIIDCTL_1_75_NS>;
 		ti,tx-internal-delay = <DP83867_RGMIIDCTL_1_75_NS>;
-		rx-skew-ps = <1750>;
-		tx-skew-ps = <1750>;
-		led-cfg1 = <0x6b90>;
-		led-cfg2 = <0x0000>;
+		ti,led-cfg1 = <0x6b90>;
+		ti,led-cfg2 = <0x0000>;
+		ti,fifo-depth = <DP83867_PHYCR_FIFO_DEPTH_4_B_NIB>;
 	};
 	sgmii_phy13: ethernet-phy@13 {
 		compatible = "ethernet-phy-id2000.a230",
